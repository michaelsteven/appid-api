{
  "env": {
    "browser": true,
    "es2021": true,
    "jest": true
  },
  "extends": ["standard"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 13,
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "semi": [2, "always"],
    "comma-dangle": "off",
    "@typescript-eslint/comma-dangle": "off"
  }
}
